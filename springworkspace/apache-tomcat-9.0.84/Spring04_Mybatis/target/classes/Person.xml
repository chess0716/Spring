 <mappers>
 	<mapper namespace="com.myspring.Person">
 	<!-- 추가 -->
	 <insert id="personInsert" parameterType="com.myspring.Person">
	 insert into person values(#(id),#(name),#(password),#(gender),#(job))
	 </insert>
 	<!-- 전체보기 -->
 </mapper>
 </mappers>